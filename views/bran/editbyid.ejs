  <!DOCTYPE html>
  <html lang="en">

  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="manifest" href="/manifest/branReport_manifest.json">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;1,800&display=swap"
      rel="stylesheet">
    <link rel="stylesheet" href=" /css/style.css">
    <title>Document</title>
  </head>

  <body>
  <%- include('../partials/navbar') -%>

  <form action="/branreport/obj_id/<%= logs[0]._id %>" method="post" autocomplete="off" class="m-0 p-0 container-fluid">
    <div class="bg-light p-2 border " id="details">
      <summary open style='list-style:none'>
        <h6 class="bg-dark rounded p-2 text-light shadow" id="headingnewlog">BRAN REPORT EDIT FORM</h6>
        <input type="text" id="toggledetails" value="1" class="d-none">
      </summary>
      <p class="p-4 pt-1 " >

        refno :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="refno" id="" value="<%= (logs[0].refno) %>"> <br>
        party* :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="party" id="" value="<%= (logs[0].party) %>"> <br>
        broker* :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="broker" id="" value="<%= (logs[0].broker) %>"> <br>
        <!-- logTime :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="logTime" id=""> <br> -->
        sampleDate* :<br><input style="background-color: bisque;border-radius: 5px;" type="date" name="sampleDate" id="" 
        value="<%= (logs[0].sampleDate?.getYear()+1900) %>-<%= ((logs[0].sampleDate?.getMonth()+1>=10)?logs[0].sampleDate?.getMonth()+1:'0'+(logs[0].sampleDate?.getMonth()+1)) %>-<%= (logs[0].sampleDate?.getDate()>10?logs[0].sampleDate?.getDate():'0'+logs[0].sampleDate?.getDate()) %>"><br>
        bags* :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="bags" id="" value="<%= (logs[0].bags) %>"> <br>
        branReport :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="branReport" id="" value="<%= (logs[0].branReport) %>"><br>
        plantBranReport :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="plantBranReport" id="" value="<%= (logs[0].plantBranReport) %>"> <br>
        naration :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="naration" id="" value="<%= (logs[0].naration) %>"> <br>
        user :<br><input style="background-color: bisque;border-radius: 5px;" type="text" name="user" id="" value="<%= (logs[0].user) %>"> <br>
        <input type="submit" value="submit" class="btn btn-primary m-4">
      </p>
    </div>
  </form>
  <form action="/branreport/obj_id/del/<%= logs[0]._id %>" method="post" class="h-0" style="height: 0px;">
  <input type="submit" value="delete" class="btn btn-dark text-danger shadow " style="position:relative;bottom: 111px;left: 200px;">
  </form>

  </body>
  </html>