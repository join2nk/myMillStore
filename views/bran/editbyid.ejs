  <!DOCTYPE html>
  <html lang="en">

  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <link rel="manifest" href="/manifest/branReport_manifest.json">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.0/dist/css/bootstrap.min.css" rel="stylesheet">
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Open+Sans:ital,wght@0,300;0,400;0,500;0,600;1,800&display=swap" rel="stylesheet">
    <link rel="stylesheet" href="/css/style.css">
    <title>Document</title>
  </head>

  <body>
    <%- include('../partials/navbar',{title:'Edit Bran Report'}) -%>
    <form action="/branreport/obj_id/<%= logs[0]._id %>" method="post" autocomplete="off"
      class="m-0 p-0 container-fluid">
      <div class="bg-light p-2 border " id="details">
    
        <% let year = (logs[0].sampleDate?.getYear()+1900) %>
        <% let month = ((logs[0].sampleDate?.getMonth()+1>=10)?logs[0].sampleDate?.getMonth()+1:'0'+(logs[0].sampleDate?.getMonth()+1)) %>
        <% let day = (logs[0].sampleDate?.getDate()>10?logs[0].sampleDate?.getDate():'0'+logs[0].sampleDate?.getDate()) %>
        <% let date = `${year}-${month}-${day}` %> 
        <%=year%>-<%=month%>-<%=day%>
        <p class="p-4 pt-1 ">
          refno :<br><input name="refno" value="<%= (logs[0].refno) %>"> <br>
          party* :<br><input name="party" value="<%= (logs[0].party) %>" required> <br>
          broker* :<br><input name="broker" value="<%= (logs[0].broker) %>" required> <br>
          <!-- logTime :<input name="logTime" id=""> <br> -->
          sampleDate* :<br><input type="date" name="sampleDate" value='<%=date%>' required ><br>
          bags* :<br><input name="bags" value="<%= (logs[0].bags) %>" inputmode="numeric"> <br>
          branReport :<br><input name="branReport" value="<%= (logs[0].branReport) %>" inputmode="numeric"><br>
          plantBranReport :<br><input name="plantBranReport" value="<%= (logs[0].plantBranReport) %>" inputmode="numeric"> <br>
          naration :<br><input name="naration" value="<%= (logs[0].naration) %>"> <br>
          user :<br><input name="user" value="<%= (logs[0].user) %>"> <br>
          <input type="submit" value="submit" class="btn btn-primary m-4">
        </p>
      </div>
    </form>
    <form action="/branreport/obj_id/del/<%= logs[0]._id %>" method="post" class="h-0" >
      <input type="submit" value="delete" class="btn btn-dark text-danger shadow delbtn" >
    </form>


  </body>

  </html>